<fieldset>
    <div class="previous title_text">
      <!-- <a href="<%=base%>/categoryPage1" class=""> -->
      <a href="#_" class="">
        <i class="fa fa-angle-left"></i> Back
      </a>
      <h2>Platform</h2>
    </div>
    <!--  <h2 class="fs-title"></h2> -->
    <div class="row">
      <div class="col-lg-4">
        <div _ngcontent-my-app-c9="" class="option mat-ripple ng-star-inserted" mat-ripple="">
          <span _ngcontent-my-app-c9="" class="text">Web <br>App</span>
          <!---->
          <div class="radio">
            <label>
              <input type="radio" value="<%=data.dataValues.web%>" class="sum" name="optionsRadios" id="web"
                required>
        
                <span class="indicator"></span>
            </label>
          </div>
        </div>
      </div>
      <div class="col-lg-4">
        <div _ngcontent-my-app-c9="" class="option mat-ripple ng-star-inserted" mat-ripple="">
          <span _ngcontent-my-app-c9="" class="text">Mobile<br> App</span>
          <!---->
          <div class="radio">
            <label>
              <input type="radio" value="<%=data.dataValues.mobile%>" class="sum" name="optionsRadios"
                id="mobile">
             
                <span class="indicator"></span>
            </label>
          </div>
        </div>
      </div>
      <div class="col-lg-4">
        <div _ngcontent-my-app-c9="" class="option mat-ripple ng-star-inserted" mat-ripple="">
          <span _ngcontent-my-app-c9="" class="text">Both</span>
          <!---->
          <div class="radio">
            <label>
              <input type="radio" value="<%=data.dataValues.both%>" class="sum" name="optionsRadios"
                id="both">

                <span class="indicator"></span>
            </label>
          </div>
        </div>
      </div>
    </div>
    <input type="button" name="next" class="next Login Typen" value="Save and Continue"
      style="background-color: #F06824;" />



  </fieldset>


  <fieldset>
    <div class="previous title_text">
      <a href="#_" class="">
        <i class="fa fa-angle-left"></i> Back </a>
      <h2>Login Type</h2>
    </div>
    <div class="row">
      <div class="col-lg-6">
        <div _ngcontent-my-app-c9="" class="option mat-ripple ng-star-inserted" mat-ripple="">
          <span _ngcontent-my-app-c9="" class="text">Email Login</span>
          <!---->
          <div class="radio">
            <label>
              <input type="radio" value="<%=data.dataValues.email%>" class="sum" name="login"
                id="emaillogin">
              <span class="indicator"></span>
            </label>
          </div>
        </div>
      </div>
      <div class="col-lg-6">
        <div _ngcontent-my-app-c9="" class="option mat-ripple ng-star-inserted" mat-ripple="">
          <span _ngcontent-my-app-c9="" class="text">Via Social Networks</span>
          <!---->
          <div class="radio">
            <label>
              <input type="radio" name="login" id="via social Networks"
                value="<%=data.dataValues.viaSocial%>" class="sum">
              <span class="indicator"></span>
            </label>
          </div>
        </div>
      </div>
      <div class="col-lg-6">
        <div _ngcontent-my-app-c9="" class="option mat-ripple ng-star-inserted" mat-ripple="">
          <span _ngcontent-my-app-c9="" class="text">2 Step Authentications</span>
          <!---->
          <div class="radio">
            <label>
              <input type="radio" name="login" value="<%=data.dataValues.twoStep%>" class="sum"
                id="2 step Authentications">
              <span class="indicator"></span>
            </label>
          </div>
        </div>
      </div>
      <div class="col-lg-6">
        <div _ngcontent-my-app-c9="" class="option mat-ripple ng-star-inserted" mat-ripple="">
          <span _ngcontent-my-app-c9="" class="text">No Signup Required</span>

          <div class="radio">
            <label>
              <input type="radio" name="login" id="No Signup Required" class="sum"
                value="<%=data.dataValues.NoSignUp%>">

              <span class="indicator"></span>
            </label>
          </div>
        </div>
      </div>
    </div>

    <input type="button" name="next" class="next btn_main" value="Save and Continue" />


  </fieldset>


  <fieldset>
    <div class="previous title_text">
      <a href="#_" class="">
        <i class="fa fa-angle-left"></i> Back </a>
      <h2 class="fs-title">Profile Creation</h2>
    </div>
    <div class="row">
      <div class="col-lg-6">
        <div _ngcontent-my-app-c9="" class="option mat-ripple ng-star-inserted" mat-ripple="">
          <span _ngcontent-my-app-c9="" class="text">Users & Accounts Required</span>
          <!---->
          <div class="radio">
            <label>
              <input type="radio" name="user" id="user & Accounts Required" class="sum"
                value="<%=data.dataValues.accrequired%>">
              <span class="indicator"></span>
            </label>
          </div>
        </div>
      </div>
      <div class="col-lg-6">
        <div _ngcontent-my-app-c9="" class="option mat-ripple ng-star-inserted" mat-ripple="">
          <span _ngcontent-my-app-c9="" class="text">Not Required</span>
          <!---->
          <div class="radio">
            <label>
              <input type="radio" name="user" id="Not Required" class="sum"
                value="<%=data.dataValues.notrequired%>">

              <span class="indicator"></span>
            </label>
          </div>
        </div>
      </div>
    </div>
